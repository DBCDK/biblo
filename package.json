{
  "name": "biblo",
  "version": "1.0.0",
  "description": "biblo.dk",
  "main": "src/main.js",
  "scripts": {
    "before:commit": "npm run lint && npm run features && npm run test",
    "build": "webpack --define process.env.NODE_ENV='\"production\"' --progress --colors -p",
    "build:dev": "webpack --progress --colors -d",
    "dev": "STANDARD_CACHE_TIME=1 parallelshell \"npm run start:watch -s\" \"npm run serve -s\"",
    "dev:remoteprofile": "COMMUNITY_SERVICE_URL=\"http://profile-i01.dbc.dk:3002/\" npm run dev",
    "dev:remoteprofile:dev": "KAFKA_TOPIC=\"ux-dev-biblo\" COMMUNITY_SERVICE_URL=\"http://uxscrum-i02.dbc.dk:3002/\" npm run dev",
    "dev:remoteprofile:localproxy": "NODE_TLS_REJECT_UNAUTHORIZED=0 NEW_RELIC_PROXY_URL=http://localhost:8888 http_proxy=http://localhost:8888 COMMUNITY_SERVICE_URL=\"http://profile-i01.dbc.dk:3002/\" npm run dev",
    "features": "STANDARD_CACHE_TIME=1 OPEN_PLATFORM_SERVICE_URL=\"http://platform-i01.dbc.dk:8080/v1/\" PORT=3334 cucumber.js --compilers js:babel/register selenium/features",
    "features:junit": "STANDARD_CACHE_TIME=1 SC_LOGLEVEL=0 PORT=3334 cucumber.js --format=json --compilers js:babel/register selenium/features | ./node_modules/.bin/cucumber-junit > feature_report.xml",
    "lint": "eslint ./src",
    "lint:checkstyle": "eslint ./src  -o report.xml -f checkstyle",
    "serve": "nodemon --exec node --no-stdin src/main.js",
    "selenium": "mocha -t 80000 selenium/selenium_test.js",
    "selenium:junit": "mocha -t 80000 selenium/selenium_test.js -R mocha-jenkins-reporter",
    "start": "node src/main.js",
    "start:watch": "webpack --progress --colors --watch -d",
    "test": "karma start --single-run=true",
    "test:watch": "karma start",
    "tail": "tail -f log.log",
    "tail:timer": "tail -f log.log | awk '(/event/ || /time_delta/ || /TIMER/) && !/result/'",
    "sonar": "sonar-runner -Dsonar.projectKey=biblo:local -Dsonar.projectName=BibloLocal -Dsonar.projectVersion=1.0 -Dsonar.sources=./src"
  },
