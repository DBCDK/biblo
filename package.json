{
  "name": "biblo",
  "version": "1.0.0",
  "description": "biblo.dk",
  "main": "src/main.js",
  "scripts": {
    "before:commit": "npm run lint && npm run features && npm run test",
    "build": "webpack --define process.env.NODE_ENV='\"production\"' --progress --colors -p",
    "build:dev": "webpack --progress --colors -d",
    "dev": "STANDARD_CACHE_TIME=1 npm-run-all -p start:watch serve",
    "dev:remoteprofile": "COMMUNITY_SERVICE_URL=\"http://profile-i01.dbc.dk:3002/\" npm run dev",
    "dev:remoteprofile:dev": "COMMUNITY_SERVICE_URL=\"http://uxscrum-i02.dbc.dk:3002/\" npm run dev",
    "dev:remoteprofile:localproxy": "NODE_TLS_REJECT_UNAUTHORIZED=0 NEW_RELIC_PROXY_URL=http://localhost:8888 http_proxy=http://localhost:8888 COMMUNITY_SERVICE_URL=\"http://profile-i01.dbc.dk:3002/\" npm run dev",
    "features": "LOG_LEVEL=off STANDARD_CACHE_TIME=1 OPEN_PLATFORM_SERVICE_URL=\"http://platform-i01.dbc.dk:8080/\" PORT=3334 ./node_modules/.bin/cucumberjs --compilers js:babel/register selenium/features --fail-fast",
    "features:junit": "LOG_LEVEL=off STANDARD_CACHE_TIME=1 SC_LOGLEVEL=0 PORT=3334 cucumber.js --format=json --compilers js:babel/register selenium/features | ./node_modules/.bin/cucumber-junit > feature_report.xml",
    "lint": "eslint ./src",
    "lint:checkstyle": "eslint ./src  -o report.xml -f checkstyle",
    "mmj:serve": ". ./mmj.env && forever -w --watchDirectory src/server --killSignal=SIGTERM src/main.js",
    "serve": "nodemon --exec node --no-stdin src/main.js",
    "start": "node src/main.js",
    "start:watch": "rm -rf ./public && webpack --progress --colors --watch -d",
    "test": "LOG_LEVEL=off jest",
    "test:ci": "JEST_JUNIT_OUTPUT=./output/jest.xml jest --ci --testResultsProcessor='jest-junit'",
    "tail": "tail -f log.log",
    "tail:timer": "tail -f log.log | awk '(/event/ || /time_delta/ || /TIMER/) && !/result/'",
    "sonar": "sonar-runner -Dsonar.projectKey=biblo:local -Dsonar.projectName=BibloLocal -Dsonar.projectVersion=1.0 -Dsonar.sources=./src"
  },
  "jest": {
    "setupFiles": [
      "./testlib/tests.setup.js"
    ],
    "roots": [
      "./src/"
    ],
    "testRegex": "(__tests__/).*test.js$",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(svg)$": "<rootDir>/__mocks__/svgMock.js",
      "\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DBCDK/biblo.git"
  },
  "keywords": [
    "biblo",
    "dbc"
  ],
  "author": "dbcdk",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/DBCDK/biblo/issues"
