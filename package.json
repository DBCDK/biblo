    "dbc-node-serviceprovider": "git://github.com/DBCDK/dbc-node-serviceprovider#stable_dist",
    "dbc-react-components": "git://github.com/DBCDK/dbc-react-components#stable_dist",
    "express": "^4.12.3",
    "express-session": "^1.11.3",
    "extract-text-webpack-plugin": "^0.8.1",
    "jade": "^1.9.2",
    "lodash": "^3.10.1",
    "newrelic": "^1.21.1",
    "node-sass": "^3.1.2",
    "passport": "^0.3.0",
