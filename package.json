    "newrelic": "^1.18.5",
    "react": "^0.13.2"
  },
  "devDependencies": {
    "babel": "^5.2.9",
    "babel-core": "^5.2.9",
    "babel-loader": "^5.0.0",
    "chai": "^2.3.0",
